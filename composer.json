    "type": "library",
    "keywords": ["backup", "mysqldump"],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {"Ifsnop\\": "src/Ifsnop/"}
    }
}
